{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "member-access": false,
    "jsx-no-multiline-js": false,
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "no-empty-interface": false,
    "max-line-length": [
      false
    ],
    "variable-name": [
      false
    ],
    "no-unused-variable": [
      true,
      "warning"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": [false],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ],
    "trailing-comma": [false]
  },
  "jsRules": {
    "arrow-parens": false,
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "ordered-imports": [false],
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ],
    "trailing-comma": [false]
  },
}
